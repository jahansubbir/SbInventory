@using Microsoft.AspNet.Identity
@{
    int loggedin=0;
    string UserId;
    HttpContext context = System.Web.HttpContext.Current;
    HttpCookie cookie = context.Request.Cookies["loginCookie"];
    if (cookie!=null)
    {
        loggedin = Convert.ToInt32(cookie["loggedIn"]);
    }
}
<div class="navbar-fixed-top nav navbar-collapse">
    @if (loggedin != 0)
    {
        <ul class="nav navbar-nav navbar-right navbar-collapse">
            @*<li>@Html.ActionLink("Register", "Register", "RPOLogin", routeValues: null, htmlAttributes: new { id = "registerLink" })</li>*@
            <li>@Html.ActionLink("Log out", "logout", "RPOLogin", routeValues: null, htmlAttributes: new { id = "loginLink", style = "color: black" })</li>
        </ul>
    }
    else
    {
        <ul class="nav navbar-nav navbar-right navbar-collapse">
            @*<li>@Html.ActionLink("Register", "Register", "RPOLogin", routeValues: null, htmlAttributes: new { id = "registerLink", style="color: black" })</li>*@
            <li>@Html.ActionLink("Log in", "Login", "RPOLogin", routeValues: null, htmlAttributes: new { id = "loginLink", style = "color: black" })</li>
        </ul>
    }
</div>
